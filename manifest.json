{
    "name": "PdfCollectExtension",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
    	"default_icon": "icon.png",
        "default_title": "PDF Collect",
        "default_popup": "popup.html"
    },
    "permissions": [
    	"storage",
    	"tabs"
    ],
	"content_scripts": [
	    {
	        "matches": ["*://*/*.pdf"],
	        "js": ["content.js"]
	    }
	]



	


}

